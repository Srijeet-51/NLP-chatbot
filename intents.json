      },
      {
        "tag": "refund",
        "patterns": ["I want a refund", "Return item", "Money back", "I need to return something"],
        "responses": ["I can help you with a refund. Please provide your order details."]
      },
      {
        "tag": "goodbye",
        "patterns": ["Bye", "See you", "Goodbye", "Talk to you later"],
        "responses": ["Goodbye! Have a great day!", "See you later."]
      }
    ]
  }
  